{
  "name": "bot-projet-mspr",
  "version": "1.0.1",
  "private": true,
  "description": "A Probot app",
  "author": "Jeremy Chauvin <jeremie.chauvin@epsi.fr>",
  "license": "ISC",
  "repository": "https://github.com/Singebob/bot-projet-mspr.git",
  "homepage": "https://github.com/Singebob/bot-projet-mspr",
  "bugs": "https://github.com/Singebob/bot-projet-mspr/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/index.js",
    "lint": "standard --fix",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "semantic-release": "^17.0.4",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  }
}
